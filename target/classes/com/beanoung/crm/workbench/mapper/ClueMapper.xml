    <select id="selectClueByIdForDetail" parameterType="string" resultMap="BaseResultMap">
        select c.id,
               c.fullname,
               dv1.value as appellation,
               u1.name   as owner,
               c.company,
               c.job,
               c.email,
               c.phone,
               c.website,
               c.mphone,
               dv3.value as state,
               dv2.value as source,
               u2.name   as create_by,
               c.create_time,
               u3.name   as edit_by,
               c.edit_time,
               c.description,
               c.contact_summary,
               c.next_contact_time,
               c.address
        from tbl_clue c
                 join tbl_user u1 on u1.id = c.owner
                 join tbl_user u2 on u2.id = c.create_by
                 left join tbl_user u3 on u3.id = c.edit_by
                 left join tbl_dic_value dv1 on dv1.id = c.appellation
                 left join tbl_dic_value dv2 on dv2.id = c.source
                 left join tbl_dic_value dv3 on dv3.id = c.state
        where c.id = #{clueId}
    </select>

    <!-- selectClueByIdForConvert -->
    <select id="selectClueByIdForConvert" parameterType="string" resultMap="BaseResultMap">
        select c.id, c.fullname, dv.value as appellation, c.company, u.name as owner
        from tbl_clue c
                 join tbl_user u on u.id = c.owner
                 left join tbl_dic_value dv on dv.id = c.appellation
        where c.id = #{id}
    </select>

    <!-- deleteClueById -->
    <delete id="deleteClueById" parameterType="string">
        delete
        from tbl_clue
        where id = #{id}
    </delete>

</mapper>